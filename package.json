{
  "name": "@bjornarvh/data-object-connect",
  "version": "1.0.0",
  "description": "React HOC used to give a React component data from an Appframe data object",
  "main": "dist/data-object-connect.umd.js",
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "node --experimental-modules rollup.mjs",
    "build:prod": "node --experimental-modules rollup.mjs --prod",
    "publish": "npm run publish:dev && npm run publish:prod",
    "publish:dev": "npx appframe publish --config appframe/dev.js",
    "publish:prod": "npx appframe publish --config appframe/prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://dev.azure.com/bvhansen/appframe-cli/_git/data-object-connect"
  },
  "keywords": [
    "appframe",
    "appframe web",
    "appframeweb",
    "react"
  ],
  "author": "Bj√∏rnar Vister Hansen <bvh@olenbetong.no>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@bjornarvh/appframe-cli": "^1.1.6",
    "dotenv": "^6.1.0",
    "minimist": "^1.2.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^6.1.1"
  }
}
