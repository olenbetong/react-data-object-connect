{
  "name": "@olenbetong/react-data-object-connect",
  "version": "3.0.2",
  "description": "React HOC used to give a React component data from an Appframe data object",
  "main": "./dist/umd/index.js",
  "module": "./es/index.js",
  "appframe": [
    {
      "fileName": "data-object-connect",
      "libraryName": "dataObjectConnect"
    },
    {
      "fileName": "data-object-hooks",
      "libraryName": "dataObjectHooks"
    },
    {
      "fileName": "data-handler",
      "libraryName": "dataHandler"
    },
    {
      "fileName": "index",
      "libraryName": "ReactDataObjectConnect"
    }
  ],
  "files": [
    "index.js",
    "dist",
    "es",
    "src"
  ],
  "scripts": {
    "build": "npx babel src -d es && npm run build:test && npm run build:prod",
    "build:test": "rollup --config",
    "build:prod": "rollup --config --configProd",
    "publish:test": "npx appframe publish --mode test --config appframe.config.js",
    "publish:prod": "npx appframe publish --mode production --config appframe.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olenbetong/react-data-object-connect"
  },
  "keywords": [
    "appframe",
    "appframe web",
    "appframeweb",
    "react"
  ],
  "author": "Bj√∏rnar Vister Hansen <bvh@olenbetong.no>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@olenbetong/appframe-cli": "^2.0.1",
    "babel-eslint": "^10.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "prettier": "^1.17.1",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-virtual": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
