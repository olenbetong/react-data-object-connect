{
  "name": "@olenbetong/react-data-object-connect",
  "version": "4.0.2",
  "description": "React HOC used to give a React component data from an Appframe data object",
  "main": "./dist/umd/index.js",
  "module": "./es/index.js",
  "sideEffects": false,
  "appframe": [
    {
      "fileName": "connect",
      "libraryName": "dataObjectConnect"
    },
    {
      "fileName": "hooks",
      "libraryName": "dataObjectHooks"
    },
    {
      "fileName": "dataHandler",
      "libraryName": "dataHandler"
    },
    {
      "fileName": "index",
      "libraryName": "ReactDataObjectConnect"
    }
  ],
  "files": [
    "index.js",
    "dist",
    "es",
    "src"
  ],
  "scripts": {
    "build": "npm run build:es && npm run build:test && npm run build:prod",
    "build:es": "npx babel src -d es",
    "build:test": "rollup --config",
    "build:prod": "rollup --config --configProd",
    "publish:test": "npx appframe publish --mode test --config appframe.config.js",
    "publish:prod": "npx appframe publish --mode production --config appframe.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olenbetong/react-data-object-connect"
  },
  "keywords": [
    "appframe",
    "appframe web",
    "appframeweb",
    "react"
  ],
  "author": "Bj√∏rnar Vister Hansen <bvh@olenbetong.no>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@olenbetong/appframe-cli": "^2.0.2",
    "babel-eslint": "^10.0.2",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "prettier": "^1.18.2",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-virtual": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.4"
  }
}